    let bestPriceWrapper = document.querySelector(".best-price-content .money");
    
    if (discountPrice > 0) {
      mprSizeWrapper.innerHTML = discountPrice;
    } 
    bestPriceWrapper.innerHTML = " Rs: " + " " +discountPrice;
  }, 1600);
}  

