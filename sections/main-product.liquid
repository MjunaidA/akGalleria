      siblingSpans.forEach((span) => {
        span.classList.add('font-bold');
      });

      let siblingSize = this.parentNode.querySelector('.sizes').getAttribute('sizeValue').trim();
      sizeVariants.forEach((input) => {
        let sizeValue = input.value.trim().toLowerCase();
        if (sizeValue === siblingSize) {
          input.click();
        }
      });
    });
  });

  // Variant/checkbox functionality
  sizeVariants.forEach((input) => {
    input.addEventListener('click', function () {
      let variantValue = this.value.trim().toLowerCase();
      valueRows.forEach((row) => {
        let valSize = row.querySelector('.sizes');
        let valAttr = valSize.getAttribute('sizeValue').trim();
        if (variantValue === valAttr) {
          let checkbox = valSize.parentNode.querySelector('input');
          checkbox.click();
        }
      });
      variantPriceNumber()
      variantPricePercentage();
      mrpPrice();
      // bestPrice();
      selectMessage.classList.add("hidden")
    });
  });

  // Garment Measurements Text
  sizeInfoDivs.forEach((div) => {
    let spans = div.querySelectorAll('span');
    let spanCount = spans.length;
    div.classList.add('chart-flex' + spanCount);
  });

  let specsWrap = document.querySelector('.specs-wrap');
  let specsShowMore = document.querySelector('.specs-showmore-btn');
  let specsWrapHeight = specsWrap.getBoundingClientRect().height;

  let offerWrap = document.querySelector('.offer-wrap');
  let offerShowMore = document.querySelector('.offer-showmore-btn');
  let offerWrapHeight = offerWrap.getBoundingClientRect().height;



  if (specsWrapHeight >= 300) {
    specsShowMore.classList.remove('hidden');
    specsShowMore.addEventListener('click', function () {
      specsWrap.classList.remove('mx-custom-height');
      this.classList.add('hidden');
    });
  }

  if (offerWrapHeight >= 400) {
    offerShowMore.classList.remove('hidden');
    offerShowMore.addEventListener('click', function () {
      offerWrap.classList.remove('offer-mx-custom-height');
      this.classList.add('hidden');
    });
  }


  let countrycol = document.querySelectorAll('.country-col');
  let infoHeadingRow = document.querySelectorAll('.info-wrapper .info-heading-row .sizes');
  let infoValueRow = document.querySelectorAll('.info-wrapper .value-row .sizes');


countrycol.forEach((country) => {
  let countryInput = country.querySelector("input"); 
  countryInput.addEventListener('change', () => {
    if (countryInput.checked) {
      // console.log('Input is checked:', countryInput.value);
      infoValueRow.forEach((infoValue) => {
        // console.log("infoValue",infoValue)
        infoAttr = infoValue.getAttribute("country_size");
        // console.log("infoAttr",infoAttr)
        if (countryInput.value == infoAttr){
          infoValue.classList.remove("hidden")
        }
      })
      infoHeadingRow.forEach((infoHead) => {
        infoAttr = infoHead.getAttribute("country_size");
        // console.log("infoAttr",infoAttr)
        if (countryInput.value == infoAttr){
          infoHead.classList.remove("hidden")
        }
      })
    }
    else {
    infoValueRow.forEach((infoValue) => {
      let infoAttr = infoValue.getAttribute("country_size");
      if (countryInput.value == infoAttr) {
        infoValue.classList.add("hidden");
      }
    });
    infoHeadingRow.forEach((infoHead) => {
      let infoAttr = infoHead.getAttribute("country_size");
      if (countryInput.value == infoAttr) {
        infoHead.classList.add("hidden");
      }
    });
    } 
  });
});





</script>




<script>

  
document.addEventListener('DOMContentLoaded', function() {
  const addToCartButton = document.querySelector('.product-form__submit');
  const variantRadios = document.querySelectorAll('.variant-picker .product-form__input input[type="radio"]');

  addToCartButton.addEventListener('click', function(event) {
    let variantSelected = false;

    variantRadios.forEach((radio) => {
      if (radio.checked) {
        variantSelected = true;
        selectMessage.classList.add("hidden")
      }
    }); 

    if (!variantSelected) {
      event.preventDefault(); 
      selectMessage.classList.remove("hidden")
    }
  });
});

</script>
