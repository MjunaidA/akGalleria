  function variantPriceNumber(){ 
    setTimeout(function() {
      let salePriceElement = document.querySelector(".price__sale .discounted-price .money");
      let regularPriceElement = document.querySelector(".price__sale .regular-price .money");
      let saveWrapper = document.querySelector(".save-wrapper .save-content");
      let salePriceText = salePriceElement ? salePriceElement.textContent : "";
      let regularPriceText = regularPriceElement ? regularPriceElement.textContent : "";
      let salePrice = parseFloat(salePriceText.replace(/[^\d.-]/g, ""));
      let regularPrice = parseFloat(regularPriceText.replace(/[^\d.-]/g, ""));
      let calculatedData = "(" + "Rs" + (regularPrice - salePrice) +" "+ "OFF" + ")"
      saveWrapper.innerHTML = "(" + "Rs" + (regularPrice - salePrice) +" "+ "OFF" + ")";
      saveWrapper.innerHTML = calculatedData;


    }, 1600);
  }


  function variantPricePercentage(){ 
    setTimeout(function() {
    let salePriceElement = document.querySelector(".price__sale .discounted-price .money");
    let regularPriceElement = document.querySelector(".price__sale .regular-price .money");
    let saveWrapper = document.querySelector(".save-percentage-wrapper .save-content");
    let salePriceText = salePriceElement ? salePriceElement.textContent : "";
    let regularPriceText = regularPriceElement ? regularPriceElement.textContent : "";
    let salePrice = parseFloat(salePriceText.replace(/[^\d.-]/g, ""));
    let regularPrice = parseFloat(regularPriceText.replace(/[^\d.-]/g, ""));
    
    if (regularPrice > 0) {
      let discountPercentage = ((regularPrice - salePrice) / regularPrice) * 100;
      let roundedPercentage = Math.round(discountPercentage);
      saveWrapper.innerHTML = "(" + roundedPercentage + "% OFF)";
    } else {
      saveWrapper.innerHTML = "(0% OFF)";
    }
  }, 1600);
}  


  function mrpPrice(){ 
